[advice]
    detachedhead = false
    ignoredhook = false
[color]
    ui = auto
[core]
    excludesfile = ~/.gitignore
[init]
    defaultbranch = main
[fetch]
    prune = true
[gc]
    pruneexpire = now
    reflogexpire = now
    reflogexpireunreachable = now
    rerereresolved = now
    rerereunresolved = now
[pull]
    ff = only
[push]
    default = matching
[rebase]
    autosquash = true
    autostash = true
[user]
    email = rom.duplain@gmail.com
    name = Rom DuPlain
